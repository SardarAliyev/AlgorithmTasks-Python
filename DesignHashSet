class MyHashSet:

    def __init__(self):
       self.MySet=set()
    
    def add(self,key):
       self.MySet.add(key)


    def remove(self,key):
        self.MySet.discard(key)

    def contains(self,key):
        if key in self.MySet:
            return True
        else:
            return False

obj=MyHashSet()
obj.add(1)
obj.add(2)
print(obj.MySet)
print(obj.contains(1))